{
  "name": "gitstatus",
  "word": "identifier",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "header"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "file"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "header": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "SYMBOL",
          "name": "header_type"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "header_type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "branch"
        }
      ]
    },
    "branch": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "branch"
        },
        {
          "type": "STRING",
          "value": "."
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "oid"
            },
            {
              "type": "SYMBOL",
              "name": "head"
            },
            {
              "type": "SYMBOL",
              "name": "upstream"
            },
            {
              "type": "SYMBOL",
              "name": "ab"
            }
          ]
        }
      ]
    },
    "oid": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "oid"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "sha1"
            },
            {
              "type": "STRING",
              "value": "(initial)"
            }
          ]
        }
      ]
    },
    "head": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "head"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "branch_name"
            },
            {
              "type": "STRING",
              "value": "(detached)"
            }
          ]
        }
      ]
    },
    "upstream": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "upstream"
        },
        {
          "type": "SYMBOL",
          "name": "upstream_branch"
        }
      ]
    },
    "ab": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "ab"
        },
        {
          "type": "SYMBOL",
          "name": "ahead"
        },
        {
          "type": "SYMBOL",
          "name": "behind"
        }
      ]
    },
    "file": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "type"
        }
      ]
    },
    "type": {
      "type": "PATTERN",
      "value": "[12u]"
    },
    "ahead": {
      "type": "PATTERN",
      "value": "\\+\\d+"
    },
    "behind": {
      "type": "PATTERN",
      "value": "\\-\\d+"
    },
    "branch_name": {
      "type": "PATTERN",
      "value": "\\w+\\.?\\w+"
    },
    "upstream_branch": {
      "type": "PATTERN",
      "value": "\\w+\\/\\w+\\.?\\w*"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-z]+"
    },
    "sha1": {
      "type": "PATTERN",
      "value": "[0-9a-f]{5,40}"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

