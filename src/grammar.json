{
  "name": "gitstatus",
  "word": "identifier",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_definition"
      }
    },
    "_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "header_definition"
        }
      ]
    },
    "header_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "SYMBOL",
          "name": "header_type"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "header_type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "branch"
        }
      ]
    },
    "branch": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "branch"
        },
        {
          "type": "STRING",
          "value": "."
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "oid"
            },
            {
              "type": "SYMBOL",
              "name": "head"
            }
          ]
        }
      ]
    },
    "oid": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "oid"
        },
        {
          "type": "SYMBOL",
          "name": "data_oid"
        }
      ]
    },
    "head": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "head"
        },
        {
          "type": "SYMBOL",
          "name": "data_head"
        }
      ]
    },
    "data_oid": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "sha1"
        },
        {
          "type": "STRING",
          "value": "(initial)"
        }
      ]
    },
    "data_head": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "branch_name"
        },
        {
          "type": "STRING",
          "value": "(detached)"
        }
      ]
    },
    "branch_name": {
      "type": "PATTERN",
      "value": "[a-zA-Z]+"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-z]+"
    },
    "sha1": {
      "type": "PATTERN",
      "value": "[0-9a-f]{5,40}"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

