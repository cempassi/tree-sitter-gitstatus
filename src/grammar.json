{
  "name": "gitstatus",
  "word": "identifier",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_definition"
      }
    },
    "_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "header_definition"
        }
      ]
    },
    "header_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "header_branch"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "header_branch": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "#[ \t]*branch"
          },
          "named": false,
          "value": "#branch"
        },
        {
          "type": "STRING",
          "value": "."
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "header_branch_name"
          }
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "sha1"
          }
        }
      ]
    },
    "header_branch_name": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "oid"
        },
        {
          "type": "STRING",
          "value": "head"
        },
        {
          "type": "STRING",
          "value": "upstream"
        },
        {
          "type": "STRING",
          "value": "ab"
        }
      ]
    },
    "header_arg": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": -1,
        "content": {
          "type": "REPEAT1",
          "content": {
            "type": "PATTERN",
            "value": ".|\\\\\\r?\\n"
          }
        }
      }
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-z]+"
    },
    "sha1": {
      "type": "PATTERN",
      "value": "[0-9a-f]{5,40}"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

