    "behind": {
      "type": "PATTERN",
      "value": "\\-\\d+"
    },
    "_opt_off": {
      "type": "STRING",
      "value": "."
    },
    "branchName": {
      "type": "PATTERN",
      "value": "\\w+\\.?\\w+"
    },
    "upBranch": {
      "type": "PATTERN",
      "value": "\\w+\\/\\w+\\.?\\w*"
    },
    "sha1": {
      "type": "PATTERN",
      "value": "[0-9a-f]{5,40}"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-z]+"
